$('#photos').html("<%= j render 'photos_list' %>")
<% if @space.photos.blank? %>
  $('#photo_check').hide();
<% end %>

<% if !@space.active && !@space.price.blank? && !@space.listing_name.blank? && !@space.photos.blank? && !@space.address.blank? %>
    $('#publish_button').attr('disabled', false);
<% else %>
    $('#publish_button').attr('disabled', true);
<% end %>